@page "/Recipe"
@using Blazor.Blog.Shared.Enum

<Article_List Type="ArticleTypeEnum.Recipe" CurrentPage="@CurrentPage" Keyword="@Keyword"></Article_List>

@code {
    [Parameter]
    [SupplyParameterFromQuery]
    public int? CurrentPage { get; set; }

    [Parameter]
    [SupplyParameterFromQuery]
    public string? Keyword { get; set; }
}